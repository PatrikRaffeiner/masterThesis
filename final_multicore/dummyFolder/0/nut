FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.000148;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.000148;
    }
    primitive
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    defaultFaces
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
}

